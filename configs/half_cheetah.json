{
    "env": "half_cheetah",
    "agent": {
        "name": "dropout_mbpo",
        "model": "dropout_mbpo_model",
        "controller": "SACPolicy",
        "dynamics_lr_gamma": 1,
        "dynamics_lr": 1e-3,
        "ensemble_size": 7,
        "elite_size": 5,
        "train_batch_size": 256,
        "model_train_freq": 1000,
        "max_epochs_since_update": 5,
        "lr_scheduler": [40, 70, 120, 140, 150],
        "dynamics_hidden_size": 200,
        "dynamics_weight_decay_rate": 1,
        "dynamics_type": "bootstrap",
        "fitting_error_correction": true,
        "fit_input": true,
        "fec_length": 2,
        "fec_decay": 0.2,
        "fec_logsumexp": true,
        "penalty": false,
        "dropout": 0.05,
        "dropout_mask_nums": 5,
        "aleatoric": "no",
        "SACPolicy": {
            "rollout_batch_size": 100000,
            "gamma": 0.99,
            "tau": 0.005,
            "alpha": 0.2,
            "target_update_interval": 1,
            "automatic_entropy_tuning": true,
            "target_entropy": "auto",
            "replay_size": 1000000,
            "model_retain_epochs": 1,
            "rollout_min_epoch": 20,
            "rollout_max_epoch": 100,
            "rollout_min_length": 2,
            "rollout_max_length": 2,
            "min_pool_size": 1000,
            "real_ratio": 0.05,
            "train_every_n_steps": 1,
            "num_train_repeat": 1,
            "num_train_data_repeat": 1,
            "epoch_train_num": 5,
            "epoch_num_train_repeat": 8000,
            "max_train_repeat_per_step": 1,
            "policy_train_batch_size": 256,
            "max_path_length": 1000
        }
    },
    "experiment": {
        "name": "MBRLSampleNew",
        "horizon": 1000,
        "random_horizon": 1000,
        "ntrain_iters": 400,
        "random_ntrain_iters": 5,
        "noise": 0,
        "step_num": 1
    },
    "test": true,
    "test_params": {
        "best_policy_margin": 1000,
        "step_real_ratio": 1,
        "fallback_train_num": 1000
    },
    "device": "cuda",
    "random_seed": 0
}
